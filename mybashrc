export PATH=/usr/local/bin:/localdisk/designer/hbai/bin:$PATH

# Load stx-builder configuration
source ${HOME}/buildrc

export PROJECT SRC_BUILD_ENVIRONMENT MYPROJECTNAME MYUNAME
export MY_BUILD_CFG MY_BUILD_CFG_RT MY_BUILD_CFG_STD MY_BUILD_DIR MY_BUILD_ENVIRONMENT MY_BUILD_ENVIRONMENT_FILE MY_BUILD_ENVIRONMENT_FILE_RT MY_BUILD_ENVIRONMENT_FILE_STD MY_DEBUG_BUILD_CFG_RT MY_DEBUG_BUILD_CFG_STD MY_REPO MY_REPO_ROOT_DIR MY_RELEASE MY_WORKSPACE LAYER
export FORMAL_BUILD=0
export PATH=$MY_REPO/build-tools:$PATH

source /usr/local/bin/finishSetup.sh
